    padding: 20px;\n          border: solid 2px rgba(0,0,0,0);\n          transition: all .3s;\n        }\n\n        input[type=\"radio\"]:hover + label {\n          cursor: pointer;\n        }\n\n        label {\n          width: 100%;\n          font-family: quicksand, arial, sans-serif;\n          text-transform: uppercase;\n          letter-spacing: 1px;\n\n          img {\n            margin: 0 auto 10px;\n            max-width: 80px;\n            display: block;\n          }\n        }\n        div {\n          align-self: stretch;\n          width: calc(50% - 30px);\n          margin: 15px;\n\n          input[type=\"radio\"]:checked + label {\n            border: solid 2px $semi-black;\n            transition: all .3s;\n          }\n        }\n      }\n\n      div.options.color {\n        input[type=\"radio\"]#red:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $red;\n          color: $white;\n        }\n\n        input[type=\"radio\"]#green:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $green;\n          color: $black;\n        }\n\n        input[type=\"radio\"]#white:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $white;\n          color: $black;\n        }\n\n        input[type=\"radio\"]#black:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $semi-black;\n          color: $white;\n        }\n\n        input[type=\"radio\"]#blue:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $blue;\n          color: $black;\n        }\n\n         input[type=\"radio\"]#pink:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $pink;\n          color: $black;\n        }\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"6c353432e69cd33869ed4ca7c5af48d2","cacheData":{"env":{}}}