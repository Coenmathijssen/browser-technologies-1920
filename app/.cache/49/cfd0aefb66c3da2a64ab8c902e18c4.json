     box-shadow: 1px 1px 0px $black,\n                 1.5px 1.5px 0px $black,\n                 2px 2px 0px $black,\n                 2.5px 2.5px 0px $black,\n                 3px 3px 0px $black,\n                 3.5px 3.5px 0px $black,\n                 4px 4px 0px $black,\n                 4.5px 4.5px 0px $black,\n                 5px 5px 0px $black,\n                 5.5px 5.5px 0px $black,\n                 6px 6px 0px $black,\n                 6.5px 6.5px 0px $black,\n                 7px 7px 0px $black;\n\n    li {\n      position: relative;\n      font-family: catamaran, arial, sans-serif;\n      font-size: 1em;\n      letter-spacing: 2px;\n      text-transform: uppercase;\n      color: $white;\n    }\n\n    a {\n      text-decoration: none;\n\n      &:nth-of-type(1), &:nth-of-type(2), &:nth-of-type(3), &:nth-of-type(4) {\n        li::after {\n          position: absolute;\n          content: '';\n          top: 30%;\n          transform: translateX(9px);\n          background: url('../img/arrow-right.svg') no-repeat;\n          background-size: 30px;\n          width: 30px;\n          height: 20px;\n          display: inline-block;\n        }\n      }\n    }\n  }\n}\n","public/scss/colors.scss":"$semi-black: #232327;\n$black: #000000;\n$white: #FDFDFD;\n$red: #B10E03;\n$green: #B4EEB4;\n$blue: #2AB0B9;\n$pink: #FFB6C1;\n","public/scss/main.scss":"@import 'typography';\n@import 'colors';\n@import 'navigation';\n\n.favorites {\n  max-width: 50px;\n  padding: 5px 20px;\n  position: absolute;\n  left: 3vw;\n  top: 5vh;\n  border: solid 2px $semi-black;\n  background-color: $white;\n\n  p {\n    text-decoration: none;\n    display: inline;\n    font-family: catamaran, arial, sans-serif;\n    color: $black;\n    font-weight: bold;\n  }\n}\n\nmain div {\n  display: flex;\n  width: 100%;\n  max-width: 1280px;\n  margin: 0 auto;\n\n  section.t-shirt {\n    width: calc(50% - 100px);\n    padding: 0 50px;\n\n    .shirt-button-container {\n      display: flex;\n      justify-content: space-between;\n\n      button {\n        position: relative;\n        font-family: quicksand, arial, sans-serif;\n        text-transform: uppercase;\n        letter-spacing: 1px;\n        background: none;\n        background: none;\n        color: inherit;\n        border: solid 1px $semi-black;\n\n        &:hover {\n          cursor: pointer;\n        }\n\n        &:first-of-type {\n          padding: 5px 10px 5px 20px;\n\n          &::after {\n            content: '';\n            position: absolute;\n            content: '';\n            top: -5px;\n            left: -15px;\n            transform: rotate(180deg);\n            background: url('../img/arrow-black.svg') no-repeat;\n            background-size: 30px;\n            width: 30px;\n            height: 20px;\n            display: inline-block;\n          }\n        }\n\n        &:last-of-type {\n          padding: 5px 20px 5px 10px;\n\n           &::after {\n            content: '';\n            position: absolute;\n            content: '';\n            top: 7px;\n            right: -15px;\n            background: url('../img/arrow-black.svg') no-repeat;\n            background-size: 30px;\n            width: 30px;\n            height: 20px;\n            display: inline-block;\n          }\n        }\n      }\n    }\n  }\n\n  section.choose {\n    width: calc(50% - 100px);\n    padding: 0 50px;\n\n    fieldset {\n      padding: 20px;\n      text-align: center;\n      border: 1px solid $black;\n\n      legend {\n        font-family: catamaran, arial, sans-serif;\n        font-size: 2em;\n        font-weight: bold;\n        letter-spacing: 2px;\n      }\n\n      div.options {\n        display: flex;\n        flex-direction: row;\n        justify-content: stretch;\n        align-items: center;\n        flex-wrap: wrap;\n\n        input[type=\"radio\"] {\n          opacity: 0;\n          position: absolute;\n        }\n\n        input[type=\"radio\"] + label {\n          padding: 20px;\n          border: solid 2px rgba(0,0,0,0);\n          transition: all .3s;\n        }\n\n        input[type=\"radio\"]:hover + label {\n          cursor: pointer;\n        }\n\n        label {\n          width: 100%;\n          font-family: quicksand, arial, sans-serif;\n          text-transform: uppercase;\n          letter-spacing: 1px;\n\n          img {\n            margin: 0 auto 10px;\n            max-width: 80px;\n            display: block;\n          }\n        }\n        div {\n          align-self: stretch;\n          width: calc(50% - 30px);\n          margin: 15px;\n\n          input[type=\"radio\"]:checked + label {\n            border: solid 2px $semi-black;\n            transition: all .3s;\n          }\n        }\n      }\n\n      div.options.color {\n        input[type=\"radio\"]#red:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $red;\n          color: $white;\n        }\n\n        input[type=\"radio\"]#green:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $green;\n          color: $black;\n        }\n\n        input[type=\"radio\"]#white:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $white;\n          color: $black;\n        }\n\n        input[type=\"radio\"]#black:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $semi-black;\n          color: $white;\n        }\n\n        input[type=\"radio\"]#blue:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $blue;\n          color: $black;\n        }\n\n         input[type=\"radio\"]#pink:checked + label {\n          border: solid 2px $semi-black;\n          background-color: $pink;\n          color: $black;\n        }\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"922795f295d12e952a432680551be4af","cacheData":{"env":{}}}